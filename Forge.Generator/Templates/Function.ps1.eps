function <%= $Name %> {
    <#
    .SYNOPSIS
        <%= $Description %>.

    .DESCRIPTION
        <%= $Description %>.

    .EXAMPLE
        <%= $Name %> -Name MyName 

    .PARAMETER Name
        TODO: Name parameter description.

    .PARAMETER Path
        TODO: Path parameter description.

<% $Parameters | ForEach { -%>
    .PARAMETER <%= $_ %>
        TODO: <%= $_ %> parameter description.

<% } -%>
    #>
    [CmdletBinding(SupportsShouldProcess = $true, ConfirmImpact='Low')]
    Param(
        [Parameter(Mandatory = $true)]
        [String]$Name,

        [String]$Path = "$Name"<%= if ($Parameters) { "," } %>
<% $Parameters | ForEach { $i = 1 } { -%>

        $<%= $_ %><%= if ($i++ -lt $Parameters.length) { ",`n" } %>
<% } -%>
    )
    Begin {
        Initialize-ForgeContext -SourceRoot (Join-Path $PSScriptRoot "Templates") `
            -DestinationPath $Path
    }
    Process {
        if (!$PSCmdlet.ShouldProcess($Name, "Create <%= $Name %>")) {
            return
        }

        Set-ForgeBinding @{
            Name          = $Name
            # More variables
        }

        # New-ForgeDirectory -Destination Test > $Null
        # Copy-ForgeFile -Source "README.md"
    }
}