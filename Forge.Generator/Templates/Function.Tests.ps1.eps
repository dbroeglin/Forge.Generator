Set-PSDebug -Strict
$sut = (Split-Path -Leaf $MyInvocation.MyCommand.Path) -replace '\.Tests\.', '.'
if (!(Get-Module "Forge")) {
    Import-Module "Forge"
}
. "$PSScriptRoot\..\<%= $ModuleName %>\$sut"

Describe "<%= $Name %>" {
    $Name = "TestGenerator"
    $TestPath = Join-Path $TestDrive $Name 
    $Params = @{ 
        Name   = $Name
        Path   = $TestPath
    }

    Context "-Name $Name -Path... "{
        <%= $Name %> @Params 

        It "should do something" {
            # TODO: Some assertions...
        }

    }
}
